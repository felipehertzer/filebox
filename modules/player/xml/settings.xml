<?xml version="1.0" encoding="utf-8"?>
<settings width = "256" height = "78">
	
	<autoplay>false</autoplay>
	
	<margin>5</margin>
	
	<strokeColor>0xEEEEEE</strokeColor>
	<fillColor>0xEEEEEE</fillColor>
	
	<playPauseButton>
		<backColor>0xCCCCCC</backColor>
		<backOverColor>0xEEEEEE</backOverColor>
		
		<signStrokeColor>0x4B4B4B</signStrokeColor>
		<signFillColor>0x3b5988</signFillColor>
	</playPauseButton>
	
	<volumeControl initLevel = "50">
		<backStrokeColor>0x4B4B4B</backStrokeColor>
		<backFillColor>0xE7E8E9</backFillColor>
		
		<signColor>0xE7E8E9</signColor>
		<signOverColor>0x4B4B4B</signOverColor>
		
		<percentText color = "0xE7E8E9" marginX = "5" positionY = "top" />  <!-- positionY can be: top, bottom, center or a number -->
	</volumeControl>
	
	<visualizer backColor = "0x7f98cc" wavesColor = "0xFFFFFF" />
	
	<songText>
		<artistColor>0xE7E8E9</artistColor>
		<artistFontSize>14</artistFontSize>
		
		<songColor>0xE7E8E9</songColor>
		<songFontSize>12</songFontSize>
	</songText>	
	
</settings>